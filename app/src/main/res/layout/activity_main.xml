<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.anderssonlegitapp.joakim.sensormadness.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btn_dataread"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:onClick="clickReadData"
        android:text="Read data"
        app:layout_constraintTop_toBottomOf="@+id/bnt_compass"
        tools:layout_editor_absoluteX="8dp" />

    <Button
        android:id="@+id/bnt_compass"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:onClick="clickCompass"
        android:text="Compass"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp" />
</android.support.constraint.ConstraintLayout>
